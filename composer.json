{
    "name": "gokhanakkurt/pet-savior",
    "description": "Pet Savior API",
    "keywords": ["pet", "savior", "api", "webservice"],
    "license": "MIT",
    "type": "project",
    "authors":[
        {
            "name": "Gokhan Akkurt",
            "email": "akkurtgokhan@gmail.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "nesbot/carbon": "~1.14"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
